<?xml version="1.0" encoding="UTF-8"?>
<!--
    Override the method Mage_Adminhtml_Sales_Order_InvoiceController:Capture($invoice) in the admin
    Allowing invoices to be marked paid offline!
-->
<config>
    <modules>
        <Jay_Capture>
            <active>true</active>
            <codePool>local</codePool>
        </Jay_Capture>
    </modules>
</config>
